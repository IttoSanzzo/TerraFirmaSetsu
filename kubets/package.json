{
	"name": "terrafirmasetsu-kubejs",
	"version": "7.6",
	"private": true,
	"description": "KubeJs for TerraFirmaSetsu Minecraft Modpack",
	"author": "Itto Sanzzo",
	"license": "SEE LICENSE IN LICENSE",
	"config": {
		"kubejs_out": "../../Minecraft/Instances/TerraFirmaSetsu dev 76 dev 1/kubejs"
	},
	"scripts": {
		"dev": "node builder.mjs --watch --out \"$npm_package_config_kubejs_out\"",
		"rebuild": "rm -rf \"$npm_package_config_kubejs_out\" && rm -rf ./.buildcache.json && node builder.mjs --out \"$npm_package_config_kubejs_out\"",
		"redev": "rm -rf \"$npm_package_config_kubejs_out\" && rm -rf ./.buildcache.json && node builder.mjs --watch --out \"$npm_package_config_kubejs_out\"",
		"build": "node builder.mjs --out \"$npm_package_config_kubejs_out\"",
		"bundle": "node bundler.mjs --root \"$npm_package_config_kubejs_out\"",
		"build-builder": "bun build builder.mjs --compile --outfile builder"
	},
	"devDependencies": {
		"@babel/generator": "^7.28.5",
		"@babel/parser": "^7.28.5",
		"@babel/traverse": "^7.28.5",
		"@babel/types": "^7.28.5",
		"acorn": "^8.15.0",
		"acorn-walk": "^8.3.4",
		"chokidar": "^5.0.0",
		"esbuild": "^0.27.0",
		"fs-extra": "^11.3.2",
		"glob": "^13.0.0",
		"prettier": "^3.7.3",
		"typescript": "^5.6.3"
	}
}